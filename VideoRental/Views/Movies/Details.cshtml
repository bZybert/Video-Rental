@model VideoRental.Models.Movie
@{
    ViewData["Title"] = "Details";
}

<h2>@Model.Name</h2>

<ul>
    <li>Genre: @Model.Genre.Name</li>
    @if (Model.NumberAvailable != null)
    {
        <li>Avalible numbers: @Model.NumberAvailable</li>
    }
    else if (Model.NumberInStock != null)
    {
        <li>Numbers in Stock: @Model.NumberInStock</li>
    }
    else if (Model.ReleaseDate != null)
    {
        <li>Release Date: @Model.ReleaseDate</li>
    }
</ul>

